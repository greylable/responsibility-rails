<div class="row">
  <div class="col s8 offset-s2">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Journal details</span>
        <%= simple_form_for @journal do |f| %>
            <%= f.input :cause, :label => "How are you feeling today?"%>
            <%= f.input :effect, :label => "How are you related to it?" %>
            <%= f.input :feeling, :label => "How did you feel?" %>
            <%= f.button :submit %>
        <% end %>
      </div>
    </div>
    <div class="card-action">
      <%= link_to 'back', :back %>
    </div>
  </div>
</div>